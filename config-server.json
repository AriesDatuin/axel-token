{
	"aws": {
		"upload": false,
		"dist": [
			"./production-folder"
		],
		"region": "us-west-1",
		"bucket": "your.bucket.com",
		"accessKeyId": "",
		"secretAccessKey": "",
		"cachecontrol": "max-age=315360000, no-transform, public",
		"cache": false,
		"sync": true,
		"gzip": true,
		"signatureVersion": "v3",
		"confirm": true
	},
	"git": {
		"upload": true,
		"dist": [
			"./"
		],
		"promise": "github.com/username/yourrepo",
		"repo": "https://github.com/username/yourrepo.git",
		"origin": "origin",
		"branch": "master",
		"user": "user",
		"password": "password",
		"message": [
			"Initial commit."
		],
		"timeOffset": 1000,
		"quiet": false,
		"confirm": true
	},
	"ftp": {
		"upload": false,
		"dist": [
			"./production-folder"
		],
		"host": "",
		"path": "dev/build",
		"user": "username",
		"password": "password",
		"secure": false,
		"parallel": 10,
		"maxConnections": 5,
		"timeOffset": 0,
		"idleTimeout": 100,
		"reload": true,
		"confirm": true
	}
}