{
	
	"options": {
		"site": "https://axeltoken.com",
		"production": true,
		"serviceworker": true,
		"appBanner": false,
		"sourcemaps": false,
		"previewURL": "index.html"
	},

	"paths": {
		"source": "source/",
		"build": "build/"
	},

	"ga": {
		"track": true,
		"url": "axeltoken.com",
		"uid": "UA-126179666-1",
		"anonymizeIp": true,
		"bounceTime": "",
		"demographics": true,
		"linkAttribution": false,
		"minify": true,
		"nonceTag": false,
		"require": "urlChangeTracker",
		"sendPageView": true,
		"tagPlacement": "head"
	},

	"robots": {
		"useragent": "*",
		"allow": [""],
		"disallow": ["/assets/", "/cgi-bin/", "/downloads/", "/fonts/", "/modals/", "/scripts/"]
	},

	"versioning": {
		"scripts": {
			"allow": true,
			"prefix": ".",
			"suffix": ".min"
		},
		"images": {
			"allow": false,
			"prefix": ".",
			"suffix": ""
		}
	},

	"images": {
		"paths": "assets/",

		"raster": {
			"level": 5,
			"interlaced": true,
			"progressive": true
		},

		"sprite": {
			"paths": "sprites/",
			"convert": false,
			"mode": "sprite",
			"class": "sprite-wrapper",
			"selector": "sprite",
			"layout": "vertical",
			"padding": 0,
			"asyncTransforms": false,
			"preview": false
		},

		"svg": {
			"convert": false,
			"cleanup": true,
			"minify": true,
			"removeEmptyText": true,
			"removeMetadata": true,
			"removeTitle": true,
			"removeRaster": true,
			"removeScript": true,
			"removeStyle": true,
			"removeViewBox": true
		},

		"webp": {
			"convert": true,
			"level": 0,
			"quality": 100,
			"alphaQuality": 100,
			"sharpness": 0
		}

	},

	"optimizations": {
		"autoprefix": true,
		"flexbox": true,
		"grid" : true,
		"support": ["last 2 versions", "ie >= 9", "android >= 4.4", "ios >= 7"]
	},

	"html": {
		"paths": "_pages/**/*.{html,hbs,handlebars}",
		
		"root": "_pages/",
		"layouts": "_layouts/",
		"partials": "_partials/",
		"helpers": "_helpers/",
		"data": "_data/",

		"modals": {
			"paths": "_modals/**/*.{html,hbs,handlebars}",
			"output": "modals/"
		},
		
		"inlineScripts": {
			"js": ["_scripts/*.js"],
			"css": ["css/critical.css"],
			"minify": true
		}
	},

	"scripts": {
		"output": "scripts/"
	},
	
	"vendors": {
		"paths": ["_vendors/*.js"],
		"useVendorScripts": false,
		"bundle": "vendors.js",
		"lint": false,
		"jsAttribute": "async"
	},

	"js": {
		"bundle": "app.js",
		"lint": true,
		"jsAttribute": "defer",
		
		"paths": ["<JQUERY>",
				  "lib/jquery/jquery.js",


				  "<PLUGINS>",
				  ">>>>>lib/jquery/jquery.mobile.js",
				  ">>>>>lib/jquery/plugins/jquery.easing.js",
				  "lib/assets/selectorcache.js",


				  "<FEATURE DETECTION / POLYFILLS / FALLBACK>",
				  "lib/assets/modernizr.js",
				  ">>>>>lib/polyfills/intersectobserver.js",
				  ">>>>>lib/polyfills/objectfit.js",
				  ">>>>>lib/polyfills/webp.js",


				  "<DATA>",
				  "lib/assets/cookies.js",
				  ">>>>>lib/assets/dateformat.js",
				  ">>>>>lib/assets/loadjson.js",


				  "<FORMS>",
				  ">>>>>lib/forms/login.js",


				  "<WRAPPERS>",
				  ">>>>>lib/assets/fitvids.js",
				  ">>>>>lib/assets/mediawrapper.js",


				  "<UTILITIES>",
				  ">>>>>lib/assets/backbone.js",
				  ">>>>>lib/assets/focusingly.js",
				  "lib/assets/lazyload.js",
				  "lib/assets/passiveeventlisteners.js",
				  "lib/assets/print.js",
				  "lib/assets/scrollstartstop.js",
				  ">>>>>lib/assets/smoothstate.js",
				  "lib/assets/underscore.js",
				  "lib/assets/visibility.js",
				  ">>>>>lib/assets/watch.js",


				  "<UI>",
				  "lib/assets/headroom.js",
				  ">>>>>lib/assets/menuaim.js",


				  "<FX>",
				  ">>>>>lib/assets/granim.js",
				  ">>>>>lib/particles/particles.core.js",
				  ">>>>>lib/particles/particles.app.js",
				  "lib/assets/neuralnetwork.js",


				  "<ELEMENT MANIPULATION>",
				  ">>>>>lib/assets/adaptive-background.js",
				  ">>>>>lib/assets/duotone.js",
				  ">>>>>lib/assets/tilt.js",


				  "<SCROLL MANIPULATION>",
				  ">>>>>lib/assets/scrolllock.js",
				  "lib/dzsparallaxer/dzsparallaxer.js",
				  "lib/assets/emergence.js",
				  ">>>>>lib/assets/rellax.js",
				  ">>>>>lib/assets/scrollme.js",


				  "<ANIMATION ENGINE (GSAP)>",
				  "lib/gsap/TweenMax.js",
				  ">>>>>lib/gsap/TimelineMax.js",
				  ">>>>>lib/gsap/plugins/BezierPlugin.js",
				  ">>>>>lib/gsap/plugins/ColorPropsPlugin.js",
				  ">>>>>lib/gsap/plugins/CSSPlugin.js",
				  ">>>>>lib/gsap/plugins/CSSRulePlugin.js",
				  "lib/gsap/plugins/DirectionalRotationPlugin.js",
				  ">>>>>lib/gsap/plugins/ModifiersPlugin.js",
				  "lib/gsap/plugins/ScrollToPlugin.js",
				  ">>>>>lib/gsap/plugins/TextPlugin.js",

				  "<Club GreenSock Plugins>",
				  ">>>>>lib/gsap/plugins/DrawSVGPlugin.js",
				  ">>>>>lib/gsap/plugins/MorphSVGPlugin.js",
				  ">>>>>lib/gsap/plugins/Physics2DPlugin.js",
				  ">>>>>lib/gsap/plugins/PhysicsPropsPlugin.js",
				  ">>>>>lib/gsap/plugins/ScrambleTextPlugin.js",
				  "lib/gsap/plugins/SplitTextPlugin.js",
				  ">>>>>lib/gsap/plugins/ThrowPropsPlugin.js",


				  "<3D LIBS>",
				  ">>>>>lib/three/three.js",
				  ">>>>>lib/three/plugins/three.controls.js",


				  "<OTHER LIBS>",
				  ">>>>>lib/p5/p5.js",
				  ">>>>>lib/p5/plugins/p5.dom.js",
				  ">>>>>lib/p5/plugins/p5.sound.js",


				  "<COMPONENTS>",
				  ">>>>>lib/fullpage/plugins/dragandmove.js",
				  ">>>>>lib/fullpage/plugins/scrolloverflow.js",
				  ">>>>>lib/fullpage/fullpage.extensions.js",
				  ">>>>>lib/fullpage/fullpage.js",

				  "lib/metafizzy/flickity.js",
				  "lib/metafizzy/imagesloaded.js",
				  ">>>>>lib/metafizzy/isotope.js",
				  "lib/metafizzy/plugins/background.lazyload.js",
				  "lib/odometer/odometer.js",


				  "<WIDGETS>",
				  ">>>>>lib/smart-app-banner/smart-app-banner.js",
				  "lib/waves/waves.js",


				  "<FRAMEWORK>",
				  "lib/framework/foundation.js",
				  ">>>>>lib/framework/plugins/what-input.js",


				  "<APP>",
				  "lib/options.js",
				  "lib/detect.js",
				  "lib/animations.js",
				  "lib/scenes.js",
				  ">>>>>lib/languages/en-strings.js",
				  ">>>>>lib/languages/es-strings.js",
				  ">>>>>lib/language.js",
				  "lib/utilities.js",
				  "lib/components.js",
				  "lib/ui.js",
				  ">>>>>lib/hashnav.js",
				  "lib/notice.js",
				  "lib/pages.js",
				  ">>>>>lib/transition.js",
				  "lib/app.js"
				 ]
	
	},
	
	"css": {
		"bundle": "app.css",
		"lint": false,
		
		"paths": ["<INIT>",
				  "css/options.css",
				  "fonts/fontawesome.css",


				  "<JQUERY>",
				  ">>>>>lib/jquery/jquery.mobile.css",


				  "<DETECTION / FALLBACKS / POLYFILLS>",


				  "DATA>",
				  "FORMS>",
				  "WRAPPERS>",
				  "UTILITIES>",
				  "UI",


				  "<FX>",


				  "<DOM>",


				  "<SCROLL MANIPULATION>",
				  "lib/dzsparallaxer/dzsparallaxer.css",


				  "<3D>",
				  "<OTHER>",


				  "<COMPONENTS>",
				  ">>>>>lib/fullpage/fullpage.css",
				  ">>>>>lib/fullpage/nav.css",
				  "lib/metafizzy/flickity.css",
				  "lib/odometer/odometer-theme-minimal.css",


				  "<WIDGETS>",
				  "lib/waves/waves.css",


				  "<FRAMEWORK>",
				  "lib/framework/foundation.css",
				  ">>>>>lib/framework/foundation.rtl.css",


				  "<ELEMENTS>",
				  "css/elements/*.css",


				  "<UI>",
				  "css/ui/*.css",


				  "<COMPONENTS>",
				  "css/components/*.css",


				  "<UTILITIES>",
				  "css/utilities/*.css",


				  "<APP>",
				  "css/copy.css",
				  "css/layout.css",
				  "lib/scenes/*.css",
				  "css/rules.css",
				  "css/theme.css",
				  "css/responsive.css"
				 ]
	
	},

	"sync": {
		"notify": true,

		"browsers": ["chrome"],
		"host": "",
		"https": false,
		"localOnly": false,
		"online": true,
		"port": "3000",

		"cors": true,
		"open": "external",
		"tunnel": false,
		"xip": false,

		"codeSync": true,
		"injectChanges": true,
		"watch": true,

		"ghostMode": true,

		"reloadDebounce": 0,
		"reloadDelay": 1000,
		"reloadThrottle": 0,
		
		"scrollElements": [],
		"scrollElementMapping": [],
		"scrollProportionally": true,
		"scrollRestoreTechnique": "window.name",
		"scrollThrottle": 0,

		"logConnections": true,
		"logFileChanges": false,
		"logLevel": "info",
		"timestamps": true
	}
	
}
